{
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": ">=2.0.5",
    "yiisoft/yii2-bootstrap": "*",
    "yiisoft/yii2-swiftmailer": "*",
    "linslin/yii2-curl": "^1.0",
    "cboden/ratchet": "^0.3.4",
    "bower-asset/handlebars": "^4.0",
    "bower-asset/js-cookie": "*",
    "bower-asset/noty": "*",
    "rmrevin/yii2-fontawesome": "~2.12",
    "react/event-loop": "dev-master as 0.4"
  },
  "require-dev": {
    "yiisoft/yii2-codeception": "*",
    "yiisoft/yii2-debug": "*",
    "yiisoft/yii2-gii": "*",
    "yiisoft/yii2-faker": "*"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/CyanoFresh/event-loop"
    }
